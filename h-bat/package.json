{
  "name": "h-bat",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:cross-browser": "playwright test --project=chromium,firefox,webkit",
    "test:mobile": "playwright test --project='Mobile Chrome','Mobile Safari'",
    "test:all": "tsx scripts/test-runner.ts",
    "test:unit-only": "tsx scripts/test-runner.ts -- --unit-only",
    "test:e2e-only": "tsx scripts/test-runner.ts -- --e2e-only",
    "format": "prettier --write .",
    "type-check": "tsc --noEmit",
    "supabase:dashboard": "open https://supabase.com/dashboard/project/tpfsgdeyyruydpzfxyud",
    "supabase:db": "npx supabase db --help",
    "supabase:gen-types": "npx supabase gen types typescript --project-id tpfsgdeyyruydpzfxyud > src/types/database.ts",
    "supabase:seed": "npx tsx scripts/seed-test-data.ts",
    "admin:create": "npx tsx scripts/create-admin.ts",
    "admin:list": "npx tsx scripts/create-admin.ts --list",
    "db:init": "node scripts/init-db.js",
    "db:seed": "node scripts/seed.js",
    "db:migrate": "knex migrate:latest",
    "db:rollback": "knex migrate:rollback",
    "db:reset": "npm run db:init && npm run db:seed",
    "db:backup": "node scripts/backup-db.js",
    "db:backup:create": "node scripts/backup-db.js create --compress",
    "db:backup:list": "node scripts/backup-db.js list",
    "db:backup:auto": "node scripts/backup-db.js auto",
    "db:backup:cleanup": "node scripts/backup-db.js cleanup"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.1.1",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@supabase/supabase-js": "^2.50.0",
    "@types/json2csv": "^5.0.7",
    "@types/papaparse": "^5.3.16",
    "bcryptjs": "^2.4.3",
    "better-sqlite3": "^9.2.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "csv-writer": "^1.6.0",
    "framer-motion": "^12.18.1",
    "json2csv": "^6.0.0-alpha.2",
    "knex": "^3.1.0",
    "lucide-react": "^0.517.0",
    "next": "14.1.0",
    "papaparse": "^5.5.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.58.1",
    "recharts": "^2.15.4",
    "tailwind-merge": "^3.3.1",
    "tone": "^15.1.22",
    "zod": "^3.25.67",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.53.0",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.15",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/better-sqlite3": "^7.6.8",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^4.5.2",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.20",
    "dotenv": "^16.4.7",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.1.0",
    "eslint-config-prettier": "^10.1.5",
    "jsdom": "^26.1.0",
    "playwright": "^1.53.0",
    "postcss": "^8.4.49",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.12",
    "supabase": "^1.215.2",
    "tailwindcss": "^3.4.14",
    "tsx": "^4.7.0",
    "typescript": "^5",
    "vitest": "^3.2.4"
  }
}
